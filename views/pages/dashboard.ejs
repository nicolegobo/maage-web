                  else if (riskLevel <= 4) color = '#2ecc71'; // Low-Medium
                  else if (riskLevel <= 6) color = '#f39c12'; // Medium
                  else if (riskLevel <= 7) color = '#e74c3c'; // High
                  else color = '#c0392b'; // Very High

