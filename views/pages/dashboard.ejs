    <script src="https://cdn.tailwindcss.com"></script>
    <script defer src="/public/js/maage/maage-web.js"></script>
</head>

<body>
    <link rel="stylesheet" href="../../public/css/maage/maage-web.css" />
    <div id="dashboard-wrapper" class="dashboard">
        <!-- Navbar -->
        <%- include('../navbar') %>
  <!-- Sidebar -->
  <div class="sidebar">
    <div class="menu">
        <div class="menu-item active" data-section="overview">
            <i class="fas fa-globe"></i>
            <span>Overview</span>
