        <div class="menu-item" data-section="diseases">
            <i class="fas fa-bacteria"></i>
            <span>Outbreaks</span>
        </div>
        <div class="menu-item" data-section="genomics">
            <i class="fas fa-dna"></i>
            <span>Surveillance</span>
        </div>
        <div class="menu-item" data-section="predictions">
            <i class="fas fa-chart-line"></i>
            <span>Predictions</span>
        </div>
        <div class="menu-item" data-section="datasets">
            <i class="fas fa-database"></i>
            <span>Datasets</span>
        </div>
